            </hto:id>
            <hto:name>
               <hto:MultiLanguageText>
                  <hto:languageText>
                     <hto:LanguageText>
                        <hto:text>Ermita de San Miguel</hto:text>
                        <hto:language>es</hto:language>
                     </hto:LanguageText>
                  </hto:languageText>
               </hto:MultiLanguageText>
            </hto:name>
            <hto:attractionType>
