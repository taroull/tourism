                  <hto:domainValue rdfs:resource="Almacigo_de_Arona,_arbol_monumento" />
               </hto:ReferencedValue>
            </hto:id>
            <hto:name>
               <hto:MultiLanguageText>
                  <hto:languageText>
                     <hto:LanguageText>
                        <hto:text>Almácigo de Arona, árbol monumento</hto:text>
                        <hto:language>es</hto:language>
                     </hto:LanguageText>
                  </hto:languageText>
               </hto:MultiLanguageText>
            </hto:name>
