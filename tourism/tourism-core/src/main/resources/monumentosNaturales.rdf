            <hto:name>
               <hto:MultiLanguageText>
                  <hto:languageText>
                     <hto:LanguageText>
                        <hto:text>Monumento Natural Roque de Jama</hto:text>
                        <hto:language>es</hto:language>
                     </hto:LanguageText>
                  </hto:languageText>
               </hto:MultiLanguageText>
            </hto:name>
