   <ETP:NaturalHeritage>
      <hto:attraction>
         <hto:Attraction rdf:about="http://turismo-de-tenerife.org/resource/monumentosNaturales/Camino_Los_Laureles">
            <hto:id>
               <hto:ReferencedValue>
